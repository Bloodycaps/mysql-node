<div class="container p-4">
    <div class="row">
        {{#each enlaces}}
        <div class="col-md-3">
            <div class="card text-center">
                <div class="card-body">
                    <a href="{{ url }}" target="_blank">
                        <h3 class="card-title text-uppercase">
                            {{titulo}}
                        </h3>
                    </a>
                    <p> {{ descripcion }} </p>
                    <p> {{created_at}} </p>
                    <a href="" class="btn btn-danger"> Eliminar </a>
                    <a href="" class="btn btn-dark"> Editar </a>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>